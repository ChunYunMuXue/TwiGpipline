{
  "model_path": "deepseek-ai/Janus-Pro-7B",
  "base_prompt": "A cozy wooden cabin beside a calm lake at sunrise, snow-covered pines and mountains glowing warmly",
  "part_template": "{i}-part",
  "parallel_size": 1,
  "stages": 3,
  "image_token_num": 576,
  "img_size": 384,
  "patch_size": 16,
  "channels": 8,
  "generation": {
    "cfg_weight": 5.0,
    "temperature": 1.0
  },
  "stage_prompt": {
    "use_understanding": true,
    "understanding_max_tokens": 128,
    "template": "{base_prompt}. Stage {stage}/{stages} focus: {pos}. Guidance: {guidance}"
  },
  "latent_control": {
    "enabled": true,
    "img_hidden_window": 32,
    "max_triggers_per_image": 10,
    "think_prompt_max_tokens": 200,
    "trigger": {
      "check_every": 32,
      "window": 8,
      "tau_drop": 0.05,
      "tau_var": 0.002,
      "tau_entropy": 3.5,
      "tau_sim": 0.25
    },
    "condenser": {
      "memory_tokens": 8,
      "num_heads": 8,
      "mlp_ratio": 2.0
    },
    "long_condenser": {
      "memory_tokens": 8,
      "num_heads": 8,
      "mlp_ratio": 2.0,
      "chunk_size": 256,
      "use_fp32_accum": true
    },
    "translator": {
      "hidden_ratio": 2.0,
      "max_scale": 1.0
    },
    "shaper": {
      "control_tokens": 4,
      "hidden_ratio": 2.0
    }
  },
  "train_data": {
    "root_dir": "/nfs/wenjie/wenjie_0104/data",
    "sources": [
      {
        "subdir": "Aesthetics-Part01",
        "template": "fluxdb-aesthetics-part01-{idx:05d}-of-00415.parquet",
        "start": 0,
        "end": 29
      },
      {
        "subdir": "Aesthetics-Part02",
        "template": "fluxdb-aesthetics-part02-{idx:05d}-of-00454.parquet",
        "start": 0,
        "end": 29
      },
      {
        "subdir": "Imaginative",
        "template": "fluxdb-imaginative-{idx:05d}-of-00168.parquet",
        "start": 0,
        "end": 24
      },
      {
        "subdir": "Text",
        "template": "fluxdb-text-{idx:05d}-of-00143.parquet",
        "start": 0,
        "end": 14
      }
    ]
  }
}


